<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="page.page.page101" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BuyActivity"
            android:label="@string/title_activity_buy" >
        </activity>
        <activity
            android:name=".BuyDetailActivity"
            android:label="@string/title_activity_buy_detail" >
        </activity>
        <activity
            android:name=".BuyLastActivity"
            android:label="@string/title_activity_buy_last" >
        </activity>
    </application>

</manifest>
